{
    "dev": {
        "app_function": "Extract.download_and_upload_to_s3",
        "profile_name": "default",
        "project_name": "scripts",
        "runtime": "python3.10",
        "aws_region": "us-east-1",
        "s3_bucket": "parcial-html",
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "environment_variables": {
            "S3_BUCKET": "parcial-html"
        }
    },
    "dev2": {
        "app_function": "process.lambda_handler",  
        "profile_name": "default",
        "project_name": "scripts2",
        "runtime": "python3.10",
        "aws_region": "us-east-1",
        "s3_bucket": "parcial-html",
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "environment_variables": {
            "S3_BUCKET": "parcial-html"
        },
        "events": [
            {
                "function": "process.lambda_handler",  
                "event_source": {
                    "arn": "arn:aws:s3:::parcial-html",
                    "events": [
                        "s3:ObjectCreated:*"
                    ]
                }
            }
        ]
    }
}
