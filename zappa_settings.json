{
    "dev": {
        "app_function": "Extract.download_and_upload_to_s3",
        "profile_name": "default",
        "project_name": "scripts",
        "runtime": "python3.10",
        "aws_region": "us-east-1",
        "s3_bucket": "bucket-parcial-bigdata",
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "environment_variables": {
            "S3_BUCKET": "bucket-parcial-bigdata"
        }
    },
    "dev2": {
        "app_function": "process.lambda_handler",  // Cambia "process.p" a "process.lambda_handler"
        "profile_name": "default",
        "project_name": "scripts2",
        "runtime": "python3.10",
        "aws_region": "us-east-1",
        "s3_bucket": "bucket-parcial-bigdata",
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "environment_variables": {
            "S3_BUCKET": "bucket-parcial-bigdata"
        },
        "events": [
            {
                "function": "process.lambda_handler",  // Cambia "process.p" a "process.lambda_handler"
                "event_source": {
                    "arn": "arn:aws:s3:::bucket-parcial-bigdata",
                    "events": [
                        "s3:ObjectCreated:*"
                    ]
                }
            }
        ]
    }
}
